<regex>
  <name>makelist3</name>
  <listrepeat><![CDATA[
        <title>[COLOR skyblue][makelist3.param2] - [makelist3.param3][/COLOR]</title>
        <link>$doregex[makelist2]</link>
		<thumbnail></thumbnail>
        ]]></listrepeat>
<expres>playlist:"(.*?)" name:"(.*?)" info:"(.*?)";</expres>
<page>https://github.com/xxspokiixx/testkod/raw/master/kozmicomx.txt</page>
<cookieJar></cookieJar>
